pragma zkay >=0.2.0;

contract RehomArgs {

	final address owner;
	uint@owner<+> y;
	uint@owner    z;

	constructor() public {
		owner = me;
	}

	function add_hom(uint@me x) public returns (uint@me<+>) {
		require(me == owner);
		y = addhom(x);
		return y;
	}

	function un_hom(uint@me<+> x) public returns (uint@me) {
		require(me == owner);
		z = unhom(x);
		return z;
	}
}
